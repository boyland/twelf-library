    <- meet/>-inversion A24 N6+1+N4=N2 M6 M622*M4=M6 M6<<N4=M24
    <- shifts-add-converse M3<<N2=M12 N6+1+N4=N2 S6M3 M3<<N6=S6M3 S6M3<<N4=M12
