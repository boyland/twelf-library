%{%

%% Incomplete

%theorem union-left-preserves-member* :
	forall* {S1} {S2} {S3} {N}
	forall {M:member S2 N}
	       {U:union S1 S2 S3}
	exists {M:member S3 N}
	true.

%theorem union-left-preserves-member*/L :
	forall* {S1} {S2} {S3} {N} {B}
	forall {M2:member S2 N}
	       {U:union S1 S2 S3}
               {M1?:member? S1 N B}
	exists {M3:member S3 N}
	true.

- : union-left-preserves-member* M2 U M3
    <- member?-total M1?
    <- union-left-preserves-member*/L M2 U M1? M3.

- : union-left-preserves-member*/L M2 U (member?/in M1) M3.

%}%
