    <- map/+-preserves-eq-converse C114=C225 N1=N2 _ _
    <- plus-implies-gt P2 nat`eq/ N1>N2
    <- gt-respects-eq N1>N2 N1=N2 nat`eq/ N2>N2
    <- gt-anti-reflexive N2>N2 F
